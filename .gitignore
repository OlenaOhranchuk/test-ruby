# Ignore bundler config.
/.bundle
/vendor/*

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*
tmp/*
tmp/cache/
tmp/letter_opener/
tmp/miniprofiler/
tmp/sitemaps/
!/log/.keep
!/tmp/.keep


